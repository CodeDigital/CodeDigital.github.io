{"pageProps":{"scripts":"<div>\r\n    <script src=\"/assets/js/p5/p5.min.js\"></script>\r\n    <script defer src=\"/assets/js/p5/addons/p5.sound.js\"></script>\r\n    <script defer src=\"/assets/js/p5/addons/p5.speech.js\"></script>\r\n    <script defer src=\"/assets/js/p5/addons/quicksettings.js\"></script>\r\n    <script defer src=\"/assets/js/p5/addons/p5.gui.js\"></script>\r\n    <script defer src=\"/assets/js/jquery.min.js\"></script>\r\n    <script>\r\n        const sketchWidth = function() {\r\n            if(window.innerWidth > 768){\r\n                return window.innerWidth - 20;\r\n            }else{\r\n                return window.innerWidth - 10;\r\n            }\r\n        }\r\n        const sketchHeight = function() {\r\n            if(window.innerWidth > 768){\r\n                return window.innerHeight - 235;\r\n            }else{\r\n                return window.innerHeight - 155;\r\n            }\r\n        }\r\n        console.log('test');\r\n        function endSound() {\r\n            try {\r\n                getAudioContext().close();\r\n            } catch (e) {\r\n                console.log('No Audio Context to close')\r\n            };\r\n        }\r\n\r\n        function openP5SliderPanel(){\r\n            let infoP5 = document.getElementsByClassName('p5-info')[0];\r\n            if(!infoP5.className.includes('p5-info-show')){\r\n                infoP5.className += \" p5-info-show\";\r\n                // infoP5.getElementsByTagName('p')[0].style.display = \"block\";\r\n                // document.getElementById('p5-info-slide').setAttribute('-webkit-transform', 'scaleX(-1)');\r\n                document.getElementById('p5-info-slide').getElementsByTagName('img')[0].style.display = \"none\";\r\n                document.getElementById('p5-info-slide').getElementsByTagName('img')[1].style.display = \"block\";\r\n            }else{\r\n                infoP5.className = \"p5-info\";\r\n                // infoP5.getElementsByTagName('p')[0].style.display = \"none\";\r\n                document.getElementById('p5-info-slide').getElementsByTagName('img')[1].style.display = \"none\";\r\n                document.getElementById('p5-info-slide').getElementsByTagName('img')[0].style.display = \"block\";\r\n            }\r\n        }\r\n    </script>\r\n</div>","project":{"name":"","description":"","disabled":true,"tags":[],"scripts":[],"html":"<h1>test</h1>"}},"__N_SSG":true}